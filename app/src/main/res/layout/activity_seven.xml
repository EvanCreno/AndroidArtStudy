<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_seven"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="org.tyk.android.artstudy.SevenActivity">


    <org.tyk.android.artstudy.NestedParentLayout
        android:id="@+id/nested_parent"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@color/error_color">


        <org.tyk.android.artstudy.NestedChildLayout
            android:id="@+id/nested_child"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center" />

    </org.tyk.android.artstudy.NestedParentLayout>

</LinearLayout>
